<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jungip.Second_Rank2"
    tools:ignore="MergeRootFrame" >

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:src="@drawable/second_rank_main3" />

    <Button
        android:id="@+id/bt_second_rank_main3_no"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="66dp"
        android:layout_marginLeft="70dp"
		android:background="@drawable/first_rank_main2_no"/>

    <Button
        android:id="@+id/bt_second_rank_main3_yes"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_second_rank_main3_no"
        android:layout_alignBottom="@+id/bt_second_rank_main3_no"
        android:layout_marginLeft="49dp"
        android:layout_toRightOf="@+id/bt_second_rank_main3_no"
        android:background="@drawable/first_rank_main2_yes" />

    <TextView
        android:id="@+id/tv_second_rank_main3_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/bt_second_rank_main3_yes"
        android:layout_marginRight="26dp"
        android:layout_marginTop="123dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>

