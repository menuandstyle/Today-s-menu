<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jungip.First_Rank6"
    tools:ignore="MergeRootFrame" >

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/first_rank_main7_2" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="62dp"
        android:layout_alignLeft="@+id/pen"
        android:layout_alignParentBottom="true"
        android:orientation="vertical" >

    </LinearLayout>

    <Button
        android:id="@+id/bt_first_rank_main7_2_pen"
        android:layout_width="200px"
        android:layout_height="200px"
        android:layout_above="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="68dp"
        android:background="@drawable/pen"
        android:onClick="mOnClick" />

    <Button
        android:id="@+id/bt_first_rank_main7_2_decisionend"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:background="@drawable/first_rank_main6_decisionend" />

