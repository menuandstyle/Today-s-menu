<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jungip.Second_Rank6_1"
    tools:ignore="MergeRootFrame" >
     <ImageView
     android:layout_width="fill_parent"
     android:layout_height="fill_parent"
     android:src="@drawable/first_rank_main7_1" />

     <Button
         android:id="@+id/bt_second_rank_main7_1_end"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignParentBottom="true"
         android:layout_centerHorizontal="true"
         android:layout_marginBottom="58dp"
		 android:background="@drawable/first_rank_main4_end"/>
      <WebView
        android:id="@+id/web1"
        android:layout_width="800px"
        android:layout_height="600px"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:focusable="true"
        android:focusableInTouchMode="true" />
      <Button
        android:id="@+id/calldirect1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_second_rank_main7_1_end"
        android:layout_alignBottom="@+id/bt_second_rank_main7_1_end"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:background="@drawable/call"
        android:onClick="mOnClick" />

    <Button
        android:id="@+id/btnback1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_second_rank_main7_1_end"
        android:layout_alignBottom="@+id/bt_second_rank_main7_1_end"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="23dp"
        android:background="@drawable/third_rank_main2_back"
        android:onClick="mOnClick" />
    </RelativeLayout>

