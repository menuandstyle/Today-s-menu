<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jungip.Third_rank_main2"
    tools:ignore="MergeRootFrame" >
    <ImageView 
    android:layout_width = "fill_parent"
    android:layout_height="fill_parent"
    android:src = "@drawable/third_rank_main2"/>

    <Button
        android:id="@+id/bt_third_rank_main2_next"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_third_rank_main2_back"
        android:layout_alignBottom="@+id/bt_third_rank_main2_back"
        android:layout_marginLeft="34dp"
        android:layout_toRightOf="@+id/bt_third_rank_main2_back"
        android:background="@drawable/third_rank_main2_next" />

    <Button
        android:id="@+id/bt_third_rank_main2_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bt_third_rank_main2_next"
        android:layout_alignLeft="@+id/bt_third_rank_main2_1"
        android:layout_marginBottom="60dp"
        android:background="@drawable/third_rank_main2_2" />

    <Button
        android:id="@+id/bt_third_rank_main2_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bt_third_rank_main2_2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="54dp"
        android:background="@drawable/third_rank_main2_1" />

    <Button
        android:id="@+id/bt_third_rank_main2_back"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="106dp"
        android:layout_marginLeft="78dp"
        android:background="@drawable/third_rank_main2_back" />

    <EditText
        android:id="@+id/et_third_rank_main2_input1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_third_rank_main2_1"
        android:layout_alignBottom="@+id/bt_third_rank_main2_1"
        android:layout_alignLeft="@+id/bt_third_rank_main2_back"
        android:ems="10" />

    <EditText
        android:id="@+id/et_third_rank_main2_input2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_third_rank_main2_2"
        android:layout_alignBottom="@+id/bt_third_rank_main2_2"
        android:layout_alignLeft="@+id/et_third_rank_main2_input1"
        android:ems="10" />
    
    </RelativeLayout>

