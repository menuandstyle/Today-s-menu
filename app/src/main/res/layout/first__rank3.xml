<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jungip.First_Rank3"
    tools:ignore="MergeRootFrame" >
    <ImageView 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/first_rank_main4"/>

    <Button
        android:id="@+id/bt_first_rank_main4_end"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/bt_first_rank_main4_yes"
        android:layout_alignBottom="@+id/bt_first_rank_main4_yes"
        android:layout_alignParentRight="true"
        android:layout_marginRight="60dp"
        android:background="@drawable/first_rank_main4_end" />

    <Button
        android:id="@+id/bt_first_rank_main4_yes"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="116dp"
        android:layout_marginRight="46dp"
        android:layout_toLeftOf="@+id/bt_first_rank_main4_end"
        android:background="@drawable/first_rank_main4_yes" />
    
    </RelativeLayout>

